    "alias": "form-media",
    "description": "Dcat-admin image/video input extension.",
    "type": "library",
    "keywords": [
        "dcat-admin", 
        "lake", 
        "media", 
        "form-media", 
        "extension"
    ],
    "homepage": "https://github.com/halyang92/form-media",
    "license": "MIT",
    "authors": [
        {
            "name": "halyang92",
            "email": "halyang92@github.com",
            "homepage": "https://github.com/halyang92"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "dcat/laravel-admin": "^2.0"
    },
    "autoload": {
